{
  "name": "feedback-scraper",
  "version": "0.0.0",
  "description": "Checks a forum channel for new posts and archives them.",
  "license": "MIT",
  "author": "Drake Costa <drake@saeris.gg> (https://saeris.gg)",
  "private": true,
  "main": "./src/index.ts",
  "type": "module",
  "scripts": {
    "build:apps": "tsup",
    "lint": "eslint --cache"
  },
  "dependencies": {
    "@discordkit/client": "workspace:^",
    "dotenv": "^17.0.0",
    "valibot": "^1.1.0"
  },
  "devDependencies": {
    "tsup": "^8.5.0"
  },
  "tsup": {
    "entry": [
      "./src/index.ts"
    ],
    "format": "esm",
    "noExternal": [
      "@discordkit/client"
    ],
    "minify": true,
    "treeshake": true
  }
}
