{
  "name": "@saeris/discordkit",
  "description": "A RESTapi Client for Discord",
  "version": "0.0.1",
  "license": "MIT",
  "keywords": [
    "discord",
    "api",
    "client",
    "rest",
    "trpc",
    "react-query"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/saeris/discordkit.git"
  },
  "homepage": "https://github.com/saeris/discordkit#readme",
  "bugs": {
    "url": "https://github.com/saeris/discordkit/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "engines": {
    "node": ">=18.x"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint \"./src/**/*.{j,t}s?(x)\"",
    "format": "yarn lint --fix",
    "prepublishOnly": "yarn lint && yarn test",
    "prepack": "yarn build",
    "release": "release-it",
    "test": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@anatine/zod-mock": "^3.13.2",
    "@faker-js/faker": "^8.0.2",
    "@release-it/conventional-changelog": "^7.0.1",
    "@saeris/eslint-config": "^2.5.0",
    "@swc/core": "^1.3.87",
    "@swc/jest": "^0.2.29",
    "@tanstack/react-query": "^4.35.3",
    "@testing-library/react": "^14.0.0",
    "@trpc/server": "^10.38.3",
    "@types/eslint": "^8.44.2",
    "@types/jest": "^29.5.5",
    "@types/node": "18.17.17",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "cross-fetch": "^4.0.0",
    "dotenv-quokka-plugin": "^1.0.0",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^1.3.1",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "release-it": "^16.1.5",
    "ts-node": "^10.9.1",
    "tsup": "^7.2.0",
    "type-fest": "^4.3.1",
    "typescript": "5.2.2",
    "zod": "^3.22.2"
  },
  "quokka": {
    "plugins": [
      "dotenv-quokka-plugin"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@saeris/eslint-config/base",
      "@saeris/eslint-config/jest",
      "@saeris/eslint-config/typescript",
      "@saeris/eslint-config/type-aware"
    ],
    "ignorePatterns": [
      ".next/**/*",
      "*.js",
      "*.cjs",
      "*.mjs",
      "*.d.ts"
    ]
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "none",
    "bracketSpacing": true,
    "jsxBracketSameLine": false
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "dts": true,
    "splitting": false,
    "sourcemap": true,
    "clean": true
  },
  "packageManager": "yarn@3.1.1",
  "sideEffects": false,
  "dependencies": {
    "ts-jest": "^29.1.1"
  }
}
