{
  "name": "@saeris/discordkit",
  "description": "A RESTapi Client for Discord",
  "version": "0.0.0",
  "license": "MIT",
  "keywords": [
    "discord",
    "api",
    "client",
    "rest"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/saeris/discordkit.git"
  },
  "homepage": "https://github.com/saeris/discordkit#readme",
  "bugs": {
    "url": "https://github.com/saeris/discordkit/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "engines": {
    "node": ">=14.x"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "ci": "yarn install --ignore-scripts --frozen-lockfile --silent --non-interactive",
    "build": "tsup",
    "build:docs": "fiddly",
    "lint": "yarn test:lint",
    "format": "yarn test:lint --fix",
    "prepare": "is-ci || husky install",
    "prepublishOnly": "yarn test",
    "prepack": "yarn build",
    "release": "release-it",
    "test": "yarn test:lint && yarn test:typecheck && yarn test:unit",
    "test:lint": "eslint \"./src/**/*.{j,t}s?(x)\"",
    "test:typecheck": "tsc --noEmit",
    "test:unit": "jest",
    "test:watch": "yarn test:unit --watch",
    "typecheck": "yarn test:typecheck",
    "version": "yarn build:docs"
  },
  "peerDependencies": {
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@release-it/conventional-changelog": "^5.1.1",
    "@saeris/eslint-config": "^2.2.0",
    "@types/eslint": "^8.4.3",
    "@types/jest": "^28.1.3",
    "@types/node": "18.0.0",
    "commitlint": "^17.0.3",
    "dotenv-quokka-plugin": "^1.0.0",
    "eslint": "^8.18.0",
    "fiddly": "^0.12.0",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "jest": "^28.1.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "release-it": "^15.1.0",
    "tsup": "^6.7.0",
    "type-fest": "^2.14.0",
    "typescript": "4.7.4",
    "zod": "^3.17.3"
  },
  "quokka": {
    "plugins": [
      "dotenv-quokka-plugin"
    ]
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "dts": true,
    "splitting": false,
    "sourcemap": true,
    "clean": true
  },
  "packageManager": "yarn@3.1.1",
  "sideEffects": false
}
