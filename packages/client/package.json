{
  "name": "@discordkit/client",
  "description": "A REST API Client for Discord",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Drake Costa <drake@saeris.gg> (https://saeris.gg)",
  "keywords": [
    "discord",
    "api",
    "client",
    "rest",
    "trpc",
    "react-query"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/discordkit/discordkit.git",
    "directory": "packages/client"
  },
  "homepage": "https://github.com/discordkit/discordkit#readme",
  "bugs": {
    "url": "https://github.com/discordkit/discordkit/issues"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "module": "./dist/index.js",
      "import": "./dist/index.js",
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": "./dist/index.js"
    },
    "./procedures": {
      "types": "./dist/procedures.d.ts",
      "module": "./dist/procedures.js",
      "import": "./dist/procedures.js",
      "require": {
        "types": "./dist/procedures.d.cts",
        "default": "./dist/procedures.cjs"
      },
      "default": "./dist/procedures.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "scripts": {
    "dev": "tsup ./src/index.ts ./src/procedures.ts --config ../../tsup.config.ts --watch",
    "build": "tsup ./src/index.ts ./src/procedures.ts --config ../../tsup.config.ts"
  },
  "dependencies": {
    "@discordkit/core": "workspace:^"
  },
  "peerDependencies": {
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "tsup": "^7.2.0"
  }
}
