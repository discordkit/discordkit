{
  "name": "@discordkit/client",
  "description": "A REST API Client for Discord",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Drake Costa <drake@saeris.gg> (https://saeris.gg)",
  "keywords": [
    "discord",
    "api",
    "client",
    "rest",
    "trpc",
    "react-query"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/discordkit/discordkit.git",
    "directory": "packages/client"
  },
  "homepage": "https://github.com/discordkit/discordkit#readme",
  "bugs": {
    "url": "https://github.com/discordkit/discordkit/issues"
  },
  "type": "module",
  "sideEffects": false,
  "imports": {
    "#*": [
      "./src/*",
      "./src/*.ts",
      "./src/*/index.ts"
    ]
  },
  "exports": {
    ".": "./src/index.ts",
    "./application": "./src/application/index.ts",
    "./channel": "./src/channel/index.ts",
    "./emoji": "./src/emoji/index.ts",
    "./event": "./src/event/index.ts",
    "./guild": "./src/guild/index.ts",
    "./interactions": "./src/interactions/index.ts",
    "./invite": "./src/invite/index.ts",
    "./logs": "./src/logs/index.ts",
    "./moderation": "./src/moderation/index.ts",
    "./stage": "./src/stage/index.ts",
    "./sticker": "./src/sticker/index.ts",
    "./template": "./src/template/index.ts",
    "./user": "./src/user/index.ts",
    "./voice": "./src/voice/index.ts",
    "./webhook": "./src/webhook/index.ts",
    "./package.json": "./package.json"
  },
  "scripts": {
    "lint": "eslint \"./src/**/*.{j,t}s?(x)\"",
    "build": "node ../../scripts/build/build.js",
    "clean": "node ../../scripts/build/clean.js"
  },
  "dependencies": {
    "@discordkit/core": "workspace:^"
  },
  "peerDependencies": {
    "zod": "^3.17.3"
  }
}
