{
  "name": "@discordkit/core",
  "description": "Core utility functions for Discordkit",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Drake Costa <drake@saeris.gg> (https://saeris.gg)",
  "keywords": [
    "discordkit",
    "core",
    "itilities"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/discordkit/discordkit.git",
    "directory": "packages/core"
  },
  "homepage": "https://github.com/discordkit/discordkit#readme",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup"
  },
  "peerDependencies": {
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "tsup": "^7.2.0"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "tsconfig": "./tsconfig.build.json",
    "dts": true,
    "minify": true,
    "metafile": true,
    "sourcemap": true,
    "clean": true
  }
}
